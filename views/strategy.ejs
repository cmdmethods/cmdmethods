<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('includes/head'); %>
</head>

<body>
    <%- include('includes/header'); %>

        <main id="strategy">
            <div class="grid">

                <%- include('includes/category-description', {card: cardlist[0]}); %>

                    <div id="pinned-cards" class="subgrid"></div>

                    <div class="subgrid">
                        <% cardlist.forEach((card)=> { %>
                            <% if (card.type!=="category-description" ) { %>
                                <%- include('includes/card', {card: card}); %>

                                    <% }; %>
                                        <% }); %>


                    </div>
        </main>


        <%- include('includes/footer'); %>
</body>

</html>